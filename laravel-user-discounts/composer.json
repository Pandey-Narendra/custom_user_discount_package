{
    "name": "acme/laravel-user-discounts",
    "description": "A Laravel package for managing user-level discounts with stacking rules.",
    "type": "library",
    "keywords": ["laravel", "discounts", "users"],
    "license": "MIT",
    "authors": [
        {
            "name": "naren",
            "email": "naren@example.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0 || ^12.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Acme\\UserDiscounts\\": "src/",
            "Acme\\UserDiscounts\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\UserDiscounts\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Acme\\UserDiscounts\\UserDiscountServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}